							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"  pm.response.to.have.status(200);",
									"});",
									"",
									"const jsonData = pm.response.json();",
									"",
									"pm.test(\"First name is updated correctly\", function () {",
									"  pm.expect(jsonData.firstName).to.eql(\"m\");",
									"});",
									"",
									"pm.test(\"Last name is updated correctly\", function () {",
									"  pm.expect(jsonData.lastName).to.eql(\"m\");",
									"});",
									"",
									"pm.test(\"Country is not null\", function () {",
									"  pm.expect(jsonData.country).to.not.be.null;",
									"});",
									"",
									"pm.test(\"All updated fields are of type string\", function () {",
									"  pm.expect(typeof jsonData.email).to.eql(\"string\");",
									"  pm.expect(typeof jsonData.phone).to.eql(\"string\");",
									"  pm.expect(typeof jsonData.city).to.eql(\"string\");",
